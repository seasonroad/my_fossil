nt1 = NodeType('nt1')
nt2 = NodeType('nt2')

db.session.add(nt1)
db.session.add(nt2)

n1 = Node('n1', 1)
n2 = Node('n2', 2)

db.session.add(n1)
db.session.add(n2)

n11 = Node('n11', 1, 1)
n12 = Node('n12', 1, 1)
n111 = Node('n111', 1, 3)
n112 = Node('n112', 1, 3)

db.session.add(n11)
db.session.add(n12)
db.session.add(n111)
db.session.add(n112)

db.session.commit()

#================Add Type====================#
t_k = NodeType('BioKingdom')
t_p = NodeType('BioPhylum')
t_sp = NodeType('BioSubPhylum')
t_c = NodeType('BioClass')
t_sc = NodeType('BioSubClass')
t_o = NodeType('BioOrder')
t_so = NodeType('BioSubOrder')
t_g = NodeType('BioGenus')
t_sg = NodeType('BioSubGenus')
t_n = NodeType('BioNotClear')
type_list = ['BioKingdom',
             'BioPhylum',
             'BioSubPhylum',
             'BioClass',
             'BioSubClass',
             'BioOrder',
             'BioSubOrder',
             'BioGenus',
             'BioSubGenus',
             'BioNotClear']
for t in type_list:
    nt = NodeType(t)
    db.session.add(nt)
db.session.commit()

#==============Add BioKingdom================#
t_k = db.session.query(NodeType)\
                .filter(NodeType.name=='BioKingdom')\
                .one()
k_list = {
"Plantae:"植物界",
"Animalia":"动物界",
}
for k,v in k_list.iteritems():
    nk = BioKingdom(k, name_cn=v)
    db.session.add(nk)
db.session.commit()

