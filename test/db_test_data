#================Add Type====================#
t_k = NodeType('BioKingdom')
t_p = NodeType('BioPhylum')
t_sp = NodeType('BioSubPhylum')
t_c = NodeType('BioClass')
t_sc = NodeType('BioSubClass')
t_o = NodeType('BioOrder')
t_so = NodeType('BioSubOrder')
t_g = NodeType('BioGenus')
t_sg = NodeType('BioSubGenus')
t_n = NodeType('BioNotClear')
type_list = ['BioKingdom',
             'BioPhylum',
             'BioSubPhylum',
             'BioClass',
             'BioSubClass',
             'BioOrder',
             'BioSubOrder',
             'BioGenus',
             'BioSubGenus',
             'BioNotClear']
for t in type_list:
    nt = NodeType(t)
    db.session.add(nt)
db.session.commit()

#==============Add BioKingdom================#
t_k = db.session.query(NodeType)\
                .filter(NodeType.name=='BioKingdom')\
                .one()
k_list = {
"Plantae:"植物界",
"Animalia":"动物界",
}
for k,v in k_list.iteritems():
    nk = BioKingdom(k, name_cn=v, ntype_id=t_k.id)
    db.session.add(nk)
db.session.commit()

#==============Add BioPhylum================#
t_p = db.session.query(NodeType)\
                .filter(NodeType.name=='BioPhylum')\
                .one()
p_list = {
"Pteridophyta":"蕨类植物门",
"Gymnospermae":"裸子植物门",
"Angiospermae":"被子植物门",
}
for k,v in p_list.iteritems():
    np = BioPhylum(k, name_cn=v, ntype_id=t_p.id, parent_id=)
    db.session.add(np)
db.session.commit()

